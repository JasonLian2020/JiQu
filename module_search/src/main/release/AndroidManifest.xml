<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sulikeji.pipixia.search">

    <application>
        <activity
            android:name=".mvp.ui.activity.SearchContactsActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:label="@string/search_contacts_name"
            android:screenOrientation="portrait">

        </activity>

        <!-- 在组件集成宿主时, 由于宿主已经声明了 CommonSDK 的公有 ConfigModule, 所以在这里只需要声明业务组件自己的 ConfigModule -->
        <meta-data
            android:name="com.sulikeji.pipixia.search.app.GlobalConfiguration"
            android:value="ConfigModule" />
    </application>
</manifest>